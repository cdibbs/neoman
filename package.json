{
  "name": "neoman",
  "version": "1.0.0-alpha.1",
  "description": "Neoman templating engine.",
  "license": "ISC",
  "author": "Chris Dibbern",
  "main": "dist/index.js",
  "bin": {
    "neoman": "./bin/neoman"
  },
  "repository": "https://github.com/ossplz/neoman",
  "scripts": {
    "build": "npm-run-all clean tsc copystatic copylocales",
    "builde2e": "tsc --project ./src/e2e/tsconfig.json",
    "buildunittest": "tsc --project ./tsconfig-xunit.json",
    "clean": "rimraf ./dist/* ./test/* ./coverage/*",
    "copylocales": "copyfiles -u 1 locales/**/* dist/locales",
    "copystatic": "copyfiles -u 2 src/browser-prompt/**/* dist/browser-prompt",
    "precover": "rimraf ./coverage/*",
    "cover": "nyc --all npm run unittest",
    "pree2e": "npm-run-all clean builde2e",
    "e2e": "alsatian --tap test/e2e/*.e2e.js",
    "report_coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test": "npm-run-all unittest e2e",
    "tsc": "tsc -p tsconfig-build.json",
    "ts-node": "ts-node",
    "unittest": "ts-node --project tsconfig-xunit.json src/spec-lib/runner.ts",
    "taptest": "ts-node --tap src/spec-lib/runner.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/chai": "^4.0.1",
    "@types/chai-as-promised": "^7.1.0",
    "@types/express": "^4.0.36",
    "@types/fs-extra": "^5.0.0",
    "@types/glob": "^5.0.30",
    "@types/i18n": "^0.8.0",
    "@types/launchpad": "^0.6.0",
    "@types/lodash": "^4.14.97",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.53",
    "@types/os-locale": "^2.1.0",
    "@types/sinon": "^4.0.0",
    "@types/tmp": "0.0.33",
    "@types/underscore": "^1.8.1",
    "@types/ws": "^3.0.0",
    "alsatian": "^2.2.1",
    "body-parser": "^1.17.2",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.0",
    "commandpost": "^1.3.0",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.1.3",
    "express": "^4.15.3",
    "fe.js": "^0.1.4",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "i18n": "^0.8.3",
    "inversify": "^4.1.1",
    "launchpad": "^0.7.0",
    "lodash": "^4.17.4",
    "minimatch": "^3.0.4",
    "mocha": "^4.0.1",
    "mock-fs-require-fix": "^1.0.1",
    "mockfs": "^0.1.5",
    "mountfs": "^0.5.0",
    "nested-error-stacks": "^2.0.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.4.1",
    "optimist": "^0.6.1",
    "os-locale": "^2.1.0",
    "reflect-metadata": "^0.1.10",
    "requireg": "^0.1.6",
    "rimraf": "^2.6.2",
    "rxjs": "^5.4.2",
    "simple-mapper": "^2.1.3",
    "sinon": "^4.1.2",
    "source-map-support": "^0.5.3",
    "tmp": "0.0.33",
    "ts-node": "^4.1.0",
    "typemoq": "^2.1.0",
    "typescript": "^2.4.1",
    "underscore": "^1.8.3",
    "ws": "^3.3.2"
  },
  "nyc": {
    "include": [
      "src/lib/**"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "typings",
      "**/*.d.ts",
      "**/*.test.ts",
      "**/*.spec.ts"
    ],
    "reporter": [
      "json",
      "html"
    ]
  }
}
