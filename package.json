{
  "name": "neoman",
  "version": "0.1.0",
  "repository": "https://github.com/cdibbs/neoman",
  "description": "Neoman templating engine.",
  "main": "dist/index.js",
  "bin": {
    "neoman": "./bin/neoman"
  },
  "author": "Chris Dibbern",
  "license": "ISC",
  "scripts": {
    "test": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\",\\\"project\\\":\\\"tsconfig-mocha.json\\\"} TS_NODE_CACHE=false mocha --recursive --compilers ts:ts-node/register --compilerOptions --bail src/lib/*.spec.ts src/lib/**/*.spec.ts src/e2e/*.e2e.ts",
    "cover": "nyc npm test",
    "e2e": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\",\\\"project\\\":\\\"tsconfig-mocha.json\\\"} TS_NODE_CACHE=false mocha --recursive --compilers ts:ts-node/register --compilerOptions --bail src/e2e/*.e2e.ts",
    "build": "npm-run-all tsc copystatic copylocales",
    "tsc": "tsc",
    "report_coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "copystatic": "copyfiles -u 2 src/browser-prompt/**/* dist/browser-prompt",
    "copylocales": "copyfiles -u 1 locales/**/* dist/locales"
  },
  "nyc": {
    "include": [
      "src/lib/*.ts",
      "src/lib/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "typings",
      "src/e2e/resources",
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/i-*.ts"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/chai": "^4.0.1",
    "@types/chai-as-promised": "0.0.31",
    "@types/express": "^4.0.36",
    "@types/fs-extra": "^3.0.3",
    "@types/glob": "^5.0.30",
    "@types/i18n": "^0.8.0",
    "@types/launchpad": "^0.6.0",
    "@types/mocha": "^2.2.41",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^7.0.28",
    "@types/os-locale": "^2.1.0",
    "@types/sinon": "^2.3.2",
    "@types/underscore": "^1.8.1",
    "@types/ws": "^3.0.0",
    "body-parser": "^1.17.2",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^2.2.0",
    "commandpost": "^1.1.0",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.0.1",
    "express": "^4.15.3",
    "fe.js": "^0.1.4",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "i18n": "^0.8.3",
    "inversify": "^4.1.1",
    "launchpad": "^0.7.0",
    "minimatch": "^3.0.4",
    "mocha": "^3.4.2",
    "mock-fs": "^4.4.1",
    "mock-fs-require-fix": "^1.0.1",
    "mockfs": "^0.1.5",
    "mountfs": "^0.5.0",
    "nested-error-stacks": "^2.0.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.3",
    "optimist": "^0.6.1",
    "os-locale": "^2.1.0",
    "reflect-metadata": "^0.1.10",
    "requireg": "^0.1.6",
    "rxjs": "^5.4.2",
    "sinon": "^2.3.8",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.2.0",
    "typescript": "^2.4.1",
    "underscore": "^1.8.3",
    "user-settings": "^0.1.5",
    "ws": "^3.0.0"
  }
}
